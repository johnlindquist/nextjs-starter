{
  "version": 2,
  "public": true,
  "alias": "nextjs-starter-meio.now.sh",
  "name": "nextjs-starter",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/next.server.build.js"
    }
  ]
}
