    "cookie-parser": "^1.4.3",
    "express": "^4.16.4",
    "glob": "^7.1.3",
    "http-proxy-middleware": "^0.19.1",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^7.0.2",
    "next-offline": "^3.3.1",
    "next-routes": "^1.4.2",
    "next-seo": "^1.6.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-layout-slot": "0.0.1",
    "styled-jsx": "^3.1.2",
    "styled-jsx-plugin-postcss": "^2.0.0",
    "styled-jsx-plugin-sass": "^1.0.0",
    "workbox-google-analytics": "^3.6.3"
  },
